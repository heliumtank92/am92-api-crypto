  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "compile": "tsc",
    "build:clean": "rm -rf ./dist",
    "build": "npm-run-all build:clean compile",
    "test": "echo 'Error: no test specified'",
